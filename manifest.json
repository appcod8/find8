{
  "name": "MetaverseSearch",
  "version": "1.1.0",
  "description": "Search across multiple search engines from a single interface.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
      "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
