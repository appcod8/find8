{
  "name": "Find8: Find Easily Auto Discover Search Engines (No Ads, No Tracking)",
  "version": "2.7.1",
  "description": "Quickly find search switch between multiple search engines. Find and auto add discover new search engines.",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
      "default_icon": {
      "16": "icon128.png",
      "48": "icon128.png",
      "128": "icon128.png"
    }
  },
  "omnibox": {
  "keyword": "8"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  }
}
