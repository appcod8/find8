{
  "name": "Find8",
  "version": "2.7.1",
  "description": "Search across and auto add multiple search engines from a single interface.",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "omnibox": {
  "keyword": "8"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "icon128.png"
  }
}
